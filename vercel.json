{
  "$schema": "https://openapi.vercel.sh/vercel.json",

  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "swiftgrowthdigital.com"
        }
      ],
      "destination": "https://www.swiftgrowthdigital.com/$1",
      "permanent": true
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}